<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="375.0" prefWidth="602.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.EliminarAeropuertoController">
    <center>
        <VBox alignment="CENTER" prefHeight="374.0" prefWidth="602.0" spacing="10" style="-fx-padding: 20;">
            <TableView fx:id="tablaAeropuerto" prefHeight="299.0" prefWidth="567.0">
                <columns>
                    <TableColumn fx:id="codAero" prefWidth="140.0" text="Código" />
               <TableColumn fx:id="nomAero" prefWidth="140.0" text="Nombre" />
               <TableColumn fx:id="paísAero" prefWidth="140.0" text="País" />
                    <TableColumn fx:id="cudadAero" prefWidth="140.0" text="Ciudad" />
                </columns>
            </TableView>
            <HBox alignment="CENTER" spacing="10">
                <Button onAction="#eliminarAeropuerto" text="Eliminar Aeropuerto" />
                <Button onAction="#volver" text="Volver" />
            </HBox>
        </VBox>
    </center>
</BorderPane>
